    "@elizaos/plugin-sql": "^2.0.0-alpha.3",
    "@elizaos/plugin-telegram": "^2.0.0-alpha.3",
    "@elizaos/plugin-todo": "workspace:*",
    "@elizaos/plugin-trust": "workspace:*",
    "@elizaos/plugin-vision": "workspace:*",
    "@elizaos/plugin-whatsapp": "^2.0.0-alpha.3",
    "@elizaos/plugin-xai": "^2.0.0-alpha.3",
    "@elizaos/skills": "^2.0.0-alpha.3",
    "@elizaos/tui": "^2.0.0-alpha.3",
    "chalk": "^5.6.2",
    "commander": "^14.0.3",
    "dotenv": "^17.2.3",
    "json5": "^2.2.3",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.11",
