{
  "name": "milaidy",
  "version": "2.0.0-alpha.3",
  "description": "cute agents for the acceleration",
  "keywords": [],
  "license": "MIT",
  "author": "",
  "bin": {
    "milaidy": "milaidy.mjs"
  },
  "files": [
    "assets/",
    "CHANGELOG.md",
    "dist/",
    "install.sh",
    "install.ps1",
    "LICENSE",
    "milaidy.mjs",
    "plugins.json",
    "README-header.png",
    "README.md",
    "skills/"
  ],
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./cli-entry": "./milaidy.mjs",
    "./eliza": "./dist/eliza.js",
    "./plugins.json": "./plugins.json"
  },
  "scripts": {
    "dev": "node scripts/dev-ui.mjs",
    "start": "node scripts/run-node.mjs start",
    "build": "tsdown && node --import tsx scripts/write-build-info.ts && node scripts/ui.js build && cd apps/app && bun run build && cd ../..",
    "desktop": "cd apps/app && bun run build && bun run cap:sync:electron && cd electron && bun run electron:start",
    "ios": "cd apps/app && bun run build && bun run cap:sync:ios && bun run cap:open:ios",
    "android": "cd apps/app && bun run build && bun run cap:sync:android && bun run cap:open:android",
    "tui": "bun scripts/run-node.mjs tui",
    "docker": "bash deploy/docker-setup.sh",
    "docker:up": "docker compose -f deploy/docker-compose.yml up -d milaidy-gateway",
    "docker:logs": "docker compose -f deploy/docker-compose.yml logs -f",
    "docker:stop": "docker compose -f deploy/docker-compose.yml down",
    "check": "bun run tsgo && bun run format",
    "format": "biome format .",
    "format:fix": "biome format --write .",
    "test": "node test/scripts/test-parallel.mjs",
    "test:e2e": "vitest run --config vitest.e2e.config.ts",
    "test:live": "MILAIDY_LIVE_TEST=1 vitest run --config vitest.live.config.ts",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "test:ui": "cd apps/ui && bun run test",
    "test:docker": "bash test/scripts/test-cleanup-docker.sh; for t in test-live-models-docker test-live-gateway-models-docker; do bash test/scripts/$t.sh || exit 1; done && for t in onboard-docker gateway-network-docker qr-import-docker doctor-install-switch-docker plugins-docker; do bash test/scripts/e2e/$t.sh || exit 1; done && bash test/scripts/test-cleanup-docker.sh",
    "test:install": "bash test/scripts/test-install-sh-docker.sh",
    "test:install:e2e": "bash test/scripts/test-install-sh-e2e-docker.sh",
    "milaidy": "node scripts/run-node.mjs",
    "start:eliza": "node --import tsx src/eliza.ts",
    "benchmark:server": "node --import tsx src/benchmark/server.ts",
    "ui:build": "node scripts/ui.js build",
    "ui:dev": "node scripts/ui.js dev",
    "ui:install": "node scripts/ui.js install",
    "docs:list": "node scripts/docs-list.js",
    "docs:bin": "node scripts/docs-list.js --build-bin",
    "generate:plugins": "node scripts/generate-plugin-index.js",
    "release:check": "node --import tsx scripts/release-check.ts",
    "prepack": "bun run build",
    "prepare": "command -v git >/dev/null 2>&1 && git config core.hooksPath git-hooks || exit 0"
  },
  "dependencies": {
    "@clack/prompts": "^1.0.0",
    "@elizaos/core": "workspace:*",
    "@elizaos/plugin-acp": "workspace:*",
    "@elizaos/plugin-agent-orchestrator": "workspace:*",
    "@elizaos/plugin-agent-skills": "^2.0.0-alpha.3",
    "@elizaos/plugin-anthropic": "^2.0.0-alpha.3",
    "@elizaos/plugin-bluebubbles": "^2.0.0-alpha.3",
    "@elizaos/plugin-browser": "workspace:*",
    "@elizaos/plugin-cli": "workspace:*",
    "@elizaos/plugin-code": "workspace:*",
    "@elizaos/plugin-commands": "^2.0.0-alpha.3",
    "@elizaos/plugin-computeruse": "workspace:*",
    "@elizaos/plugin-cron": "workspace:*",
    "@elizaos/plugin-directives": "^2.0.0-alpha.3",
    "@elizaos/plugin-discord": "^2.0.0-alpha.3",
    "@elizaos/plugin-edge-tts": "workspace:*",
    "@elizaos/plugin-experience": "^2.0.0-alpha.3",
    "@elizaos/plugin-form": "^2.0.0-alpha.3",
    "@elizaos/plugin-goals": "workspace:*",
    "@elizaos/plugin-google-genai": "^2.0.0-alpha.3",
    "@elizaos/plugin-groq": "^2.0.0-alpha.3",
    "@elizaos/plugin-imessage": "^2.0.0-alpha.3",
    "@elizaos/plugin-knowledge": "workspace:*",
    "@elizaos/plugin-local-embedding": "workspace:*",
    "@elizaos/plugin-mattermost": "^2.0.0-alpha.3",
    "@elizaos/plugin-mcp": "workspace:*",
    "@elizaos/plugin-msteams": "^2.0.0-alpha.3",
    "@elizaos/plugin-ollama": "^2.0.0-alpha.3",
    "@elizaos/plugin-openai": "^2.0.0-alpha.3",
    "@elizaos/plugin-openrouter": "^2.0.0-alpha.3",
    "@elizaos/plugin-pdf": "workspace:*",
    "@elizaos/plugin-personality": "^2.0.0-alpha.3",
